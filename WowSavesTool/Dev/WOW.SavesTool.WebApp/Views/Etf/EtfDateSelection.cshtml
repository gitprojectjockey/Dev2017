<div class="row" style="margin-top: 25px;">
    <div class="col-md-8 col-md-offset-2">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h4>
                    Customer on an active annual agreement, ETF applies.
                    Enter annual agreement end date to determine ETF amount
                </h4>
            </div>
            <div class="panel-body">
                <h4 id="wow-etf-date-display" class="text-center"></h4>

                <div class="row">
                    <div class="col-xs-4 col-xs-offset-4 col-sm-4 col-sm-offset-4 col-md-4 col-md-offset-4">
                        <div id="wow-etf-date-selection">
                            <input id="wow-selected-etf-date" type="hidden" value="" />
                            <div id="wow-etf-date"></div>
                        </div>

                    </div>
                   
                </div>

                <div id="wow-etf-data-container"></div>

            </div>
            <div class="panel-footer">
                <div class="btn-group">
                    <button id="wow-get-etf-button" type="button" class="btn btn-primary" style="display: none">
                        Get ETF
                    </button>
                    <button id="wow-etf-help-button" type="button" class="btn btn-default" data-toggle="modal" data-target="#wow-etf-help-modal">
                        Help
                    </button>
                    <button id="wow-etf-reset-button" type="button" class="btn btn-warning" style="display: none">
                        Reset
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

@{ Html.RenderPartial("EtfHelp"); }
